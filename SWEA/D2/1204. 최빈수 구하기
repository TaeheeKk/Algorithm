T = int(input())

for t in range(1,T+1):
    a = int(input())
    lst = list(map(int, input().split()))
    score = [0] * 101 # 0점 이상 100점 이하
    for i in lst:
        score[i] += 1
    mode = max(score) #최빈값 
    
    for i in range(100, -1, -1):  
        if score[i] == mode:
            print(f"#{t} {i}")
            break
